<?xml version="1.0" encoding="utf-8"?>


            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:baselineAligned="false"
                        android:layout_weight="2">
                        <RelativeLayout
                            android:id="@+id/ukviews1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/botandright"
                            android:orientation="horizontal"
                            >
                            <RelativeLayout
                                android:id="@+id/ukview1"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:id="@+id/ukimg1"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:layout_width="@dimen/_80sdp"
                                    android:src="@drawable/lock"
                                    android:layout_height="@dimen/_80sdp"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true" />
                                <TextView
                                    android:id="@+id/uktext1"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:text=""
                                    android:gravity="center"
                                    android:textColor="@color/TitleColor"
                                    android:layout_below="@+id/ukimg1"
                                    android:layout_alignLeft="@+id/ukimg1"
                                    android:layout_alignStart="@+id/ukimg1" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/ukviews2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            >
                            <RelativeLayout
                                android:id="@+id/ukview2"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@drawable/bottomborder"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/ukimg2"
                                    android:src="@drawable/lock"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_80sdp"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true" />
                                <TextView
                                    android:id="@+id/uktext2"
                                    android:text=""
                                    android:gravity="center"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:visibility="invisible"
                                    android:textColor="@color/TitleColor"
                                    android:layout_below="@+id/ukimg2"
                                    android:layout_alignLeft="@+id/ukimg2"
                                    android:layout_alignStart="@+id/ukimg2" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:baselineAligned="false"
                        android:layout_weight="2">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                           >
                            <RelativeLayout
                                android:id="@+id/ukview3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/botandright"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/ukimg3"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_80sdp"
                                    android:src="@drawable/lock"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    />
                                <TextView
                                    android:id="@+id/uktext3"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:text=""
                                    android:visibility="invisible"
                                    android:gravity="center"
                                    android:textColor="@color/TitleColor"
                                    android:layout_below="@+id/ukimg3"
                                    android:layout_alignLeft="@+id/ukimg3"
                                    android:layout_alignStart="@+id/ukimg3" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/ukviews3"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            >
                            <RelativeLayout
                                android:id="@+id/ukview4"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/bottomborder"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/ukimg4"
                                    android:src="@drawable/lock"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_80sdp"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    />
                                <TextView
                                    android:id="@+id/uktext4"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:text=""
                                    android:visibility="invisible"
                                    android:gravity="center"
                                    android:textColor="@color/TitleColor"
                                    android:layout_below="@+id/ukimg4"
                                    android:layout_alignLeft="@+id/ukimg4"
                                    android:layout_alignStart="@+id/ukimg4" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rvButton"
                        android:layout_width="match_parent"
                        android:baselineAligned="false"
                        android:layout_height="wrap_content"
                        android:layout_weight="2">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            >
                            <RelativeLayout
                                android:id="@+id/ukview5"
                                android:layout_width="match_parent"
                                android:background="@drawable/bottomright"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:id="@+id/ukimg5"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_80sdp"
                                    android:src="@drawable/lock"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    />
                                <TextView
                                    android:id="@+id/uktext5"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:text=""
                                    android:visibility="invisible"
                                    android:textColor="@color/TitleColor"
                                    android:gravity="center"
                                    android:layout_below="@+id/ukimg5"
                                    android:layout_alignLeft="@+id/ukimg5"
                                    android:layout_alignStart="@+id/ukimg5" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            >
                            <RelativeLayout
                                android:id="@+id/ukview6"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                >
                                <ImageView
                                    android:id="@+id/ukimg6"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_80sdp"
                                    android:src="@drawable/lock"
                                    android:layout_alignParentTop="true"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:layout_centerHorizontal="true" />
                                <TextView
                                    android:id="@+id/uktext6"
                                    android:textColor="@color/TitleColor"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:text=""
                                    android:visibility="invisible"
                                    android:gravity="center"
                                    android:layout_below="@+id/ukimg6"
                                    android:layout_alignLeft="@+id/ukimg6"
                                    android:layout_alignStart="@+id/ukimg6" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
